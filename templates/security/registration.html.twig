{# templates/registration/register.html.twig #}
{% extends 'base.html.twig' %}

{% block body %}
    {{ form_start(form) }}

    {{ form_row(form.email) }}
    {{ form_row(form.username) }}
    {{ form_row(form.password) }}
    {{ form_row(form.plainpassword) }}
   
  

    <button type="submit">Register!</button>
    {{ form_end(form) }}
{% endblock %}
